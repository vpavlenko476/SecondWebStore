@model Employee
<table class="table">
    <thead>
        <tr>
            <th scope="col">ФИО</th>
            <th scope="col">Возраст</th>
            <th scope="col">Email</th>
            <th scope="col">Телефон</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <th>@Model.FirstName<br />@Model.SecondName<br />@Model.Patronymic</th>
            <td>@Model.Age</td>
            <td>@Model.Email</td>
            <td>@Model.MobilePhone</td>
        </tr>
    </tbody>
</table>